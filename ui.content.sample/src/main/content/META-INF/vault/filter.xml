<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">

    <!-- Content Filters -->
    <filter root="/content/assetdistribution" />
    <!-- Changing the filter mode to merge will preserve any existing content on the target environment
    <filter root="/content/assetdistribution" mode="merge"/>
    -->
    <filter root="/content/dam/assetdistribution" mode="update"/>
    <filter root="/content/experience-fragments/assetdistribution"/>

    <!-- Force Policy and Template Updates -->
    <filter root="/conf/assetdistribution/settings/wcm" />
    <filter root="/conf/assetdistribution/settings/cloudconfigs" mode="merge" />

    <!-- assetdistribution Member Group-->
    <filter root="/home/groups/assetdistribution" mode="merge" />
    <!-- Sample User-->
    <filter root="/home/users/assetdistribution" />
</workspaceFilter>
